@keyframes pura-rotate {
  0% {
    transform: rotate(-1turn);
  }

  100% {
    transform: none;
  }
}

[data-pura='rotate'] {
  animation-play-state: var(--pura-play-state, unset);
  animation-name: pura-rotate;
  animation-timing-function: cubic-bezier(0.23, 1, 0.32, 1);
  animation-iteration-count: infinite;
}
